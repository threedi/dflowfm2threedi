{
  "target_layer": "orifice",
  "conversion_settings": {
    "use_snapping": true,
    "snapping_distance": 0.1,
    "create_connection_nodes": true,
    "length_source_field": "length",
    "length_fallback_value": 1.0,
    "azimuth_source_field": "",
    "azimuth_fallback_value": 90,
    "edit_channels": true
  },
  "fields": {
    "id": {
      "method": "auto",
      "source_attribute": ""
    },
    "code": {
      "method": "source_attribute",
      "source_attribute": "id"
    },
    "display_name": {
      "method": "expression",
      "source_attribute": "",
      "expression": "'Bridge '||id"
    },
    "crest_level": {
      "method": "source_attribute",
      "source_attribute": "reference_level",
      "expression": " max(\"leftlevel\",  \"rightlevel\") "
    },
    "crest_type": {
      "method": "expression",
      "source_attribute": "",
      "default_value": "NULL",
      "expression": "-- 3:  /*Broad crested*/\r\n-- 4:  /*Short crested*/\r\n\r\nCASE WHEN \"length\" >= 5 THEN 3 ELSE 4 END"
    },
    "discharge_coefficient_positive": {
      "method": "expression",
      "source_attribute": "",
      "expression": "CASE \r\n\tWHEN  \"allowedflowdir\" in ('both', 'positive') THEN  \"inletlosscoeff\" *   \"outletlosscoeff\" \r\n\tWHEN \"allowedflowdir\" in ('none', 'negative') THEN 0 \r\nEND"
    },
    "discharge_coefficient_negative": {
      "method": "expression",
      "source_attribute": "",
      "expression": "CASE \r\n\tWHEN  \"allowedflowdir\" in ('both', 'negative') THEN  \"inletlosscoeff\" *   \"outletlosscoeff\" \r\n\tWHEN \"allowedflowdir\" in ('none', 'positive') THEN 0 \r\nEND"
    },
    "material_id": {
      "method": "ignore",
      "source_attribute": ""
    },
    "friction_value": {
      "method": "source_attribute",
      "source_attribute": "friction_value"
    },
    "friction_type": {
      "method": "source_attribute",
      "source_attribute": "friction_type",
      "default_value": 2
    },
    "sewerage": {
      "method": "default",
      "source_attribute": "",
      "default_value": false
    },
    "connection_node_id_start": {
      "method": "auto",
      "source_attribute": ""
    },
    "connection_node_id_end": {
      "method": "auto",
      "source_attribute": ""
    },
    "cross_section_shape": {
      "method": "source_attribute",
      "source_attribute": "cross_section_shape",
      "default_value": "NULL"
    },
    "cross_section_width": {
      "method": "source_attribute",
      "source_attribute": "cross_section_width"
    },
    "cross_section_height": {
      "method": "source_attribute",
      "source_attribute": "cross_section_height"
    },
    "cross_section_table": {
      "method": "source_attribute",
      "source_attribute": "cross_section_table"
    },
    "tags": {
      "method": "ignore",
      "source_attribute": ""
    }
  },
  "connection_node_fields": {
    "id": {
      "method": "auto",
      "source_attribute": ""
    },
    "code": {
      "method": "expression",
      "source_attribute": "",
      "expression": "'Bridge '||\"id\""
    },
    "display_name": {
      "method": "ignore",
      "source_attribute": ""
    },
    "storage_area": {
      "method": "ignore",
      "source_attribute": "",
      "default_value": 1.0
    },
    "initial_water_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "visualisation": {
      "method": "default",
      "source_attribute": "",
      "default_value": 0
    },
    "manhole_surface_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "bottom_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "exchange_level": {
      "method": "ignore",
      "source_attribute": ""
    },
    "exchange_type": {
      "method": "default",
      "source_attribute": "",
      "default_value": 2
    },
    "exchange_thickness": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_in": {
      "method": "ignore",
      "source_attribute": ""
    },
    "hydraulic_conductivity_out": {
      "method": "ignore",
      "source_attribute": ""
    },
    "tags": {
      "method": "ignore",
      "source_attribute": ""
    }
  }
}